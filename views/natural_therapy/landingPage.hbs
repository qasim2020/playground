    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:200,300,400,500,600,700&display=swap" rel="stylesheet">
    <style media="screen">
    html, body, div, span, applet, object, iframe,
    h1, h2, h3, h4, h5, h6, p, blockquote, pre,
    a, abbr, acronym, address, big, cite, code,
    del, dfn, em, img, ins, kbd, q, s, samp,
    small, strike, strong, sub, sup, tt, var,
    b, u, i, center,
    dl, dt, dd, ol, ul, li,
    fieldset, form, label, legend,
    table, caption, tbody, tfoot, thead, tr, th, td,
    article, aside, canvas, details, embed,
    figure, figcaption, footer, header, hgroup,
    menu, nav, output, ruby, section, summary,
    time, mark, audio, video {
    	margin: 0;
    	padding: 0;
    	border: 0;
    	font-size: 100%;
    	font: inherit;
    	vertical-align: baseline;
    }
    /* HTML5 display-role reset for older browsers */
    article, aside, details, figcaption, figure,
    footer, header, hgroup, menu, nav, section {
    	display: block;
    }
    body {
    	line-height: 1;
    }
    ol, ul {
    	list-style: none;
    }
    blockquote, q {
    	quotes: none;
    }
    blockquote:before, blockquote:after,
    q:before, q:after {
    	content: '';
    	content: none;
    }
    table {
    	border-collapse: collapse;
    	border-spacing: 0;
    }

    section {
      min-height: 690px;
      overflow: hidden !important;
    }

    #landing-page {
        display: grid;
        height: 100vh;
        width: 100vw;
        grid-template-columns: 1fr 1fr 1fr;
        grid-template-rows: 1fr 1fr 1fr;
        background: url(/natural_therapy/landing-image.png) center center fixed no-repeat;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
        overflow: hidden;
    }

    #landing-page > div:nth-child(1) {
      grid-column: 1/2;
      grid-row: 1/2;

      font-family: Roboto;
      font-style: normal;
      font-weight: bold;
      font-size: 15px;
      line-height: 18px;
      color: #6FCF97;
      padding: 2rem;

      margin-right: -15rem;
    }

    #landing-page > div:nth-child(2) {
      grid-column: 2/-1;
      grid-row: 1/2;
      background: #6FCF97;
      height: fit-content;
      margin-left: auto;
      padding: 1rem 2rem;
      margin-right: 2rem;
      margin-top: 2rem;
      border-radius: 34px;

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 20px;
      line-height: 29px;
      text-align: center;
      color: #251e1e;

      align-items: center;
      cursor: pointer;
    }

    #landing-page > div:nth-child(2):hover {
      background: #6FCF87;
    }

    .cta > svg {
      margin: 1rem 0;
    }

    .sliders {
      padding-left: 2rem;
      position: fixed;
      top: 50%;
      left: 0%;
      transform: translate(0, -50%);
      z-index: 200;
    }

    .sliders > svg {
      cursor: pointer;
      margin: 4px 0;
    }

    #landing-page > div:nth-child(4) {
      grid-column: 2/3;
      grid-row: 2/3;

      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      line-height: 60px;
      text-align: center;
      color: #E0E0E0;

      white-space: nowrap;

    }

    #landing-page > div:nth-child(5) {
      grid-column: 3/-1;
      grid-row: 2/3;
      display: flex;
      flex-direction: column;
      justify-content: center;
      margin-left: auto;
      margin-right: 2rem;
      align-items: center;
    }

    .social-media-icons > div {
      cursor: pointer;
      margin: 4px 0;
    }

    #landing-page > div:nth-child(6) {
      grid-column: 1/2;
      grid-row: 3/-1;

      display: flex;
      flex-direction: column;
      justify-content: flex-end;
      padding-bottom: 2rem;
      padding-left: 2rem;

      margin-right: -15rem;

    }

    .photo-credits > p {
      transform-origin: left;
      transform: rotate(270deg);
      /* Photo by David Monje on Unsplash */
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 9px;
      line-height: 11px;

      /* Gray 4 */
      color: #BDBDBD;

    }


    #landing-page > div:nth-child(7) {
      grid-column: 2/3;
      grid-row: 3/-1;

      display: flex;
      justify-content: flex-end;
      flex-direction: column;
      align-items: center;
      padding-bottom: 2rem;
    }

    #landing-page > div:nth-child(8) {
      grid-column: 3/-1;
      grid-row: 3/-1;

      display: flex;
      justify-content: flex-end;
      padding: 0 2rem 2rem 0;
      margin-left: -20rem;
    }

    .menu-items > li{
      align-self: flex-end;
      list-style-type: none;
      padding: 2px 8px;
      border: 1px solid transparent;
      /* Services */
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 14px;
      line-height: 17px;

  /* Gray 5 */
      color: #E0E0E0;

    }

    .menu-items a {
      text-decoration: none;
      color: inherit;
    }

    .menu-items > li:hover {
      color: white;
      cursor: pointer;
      border: 1px solid white;
    }

    .d-flex {
      display: flex;
    }

    .flex-col {
      flex-direction: column;
    }

    .welcome-text > p:nth-child(1) {
      /* Welcome to */
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 49px;
      line-height: 60px;
      /* identical to box height */
      text-align: center;

      /* Gray 5 */
      color: #E0E0E0;

    }

    .welcome-text > p:nth-child(2) {
      /* NLP! */

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 141px;
      line-height: 172px;

      /* Gray 5 */
      color: #E0E0E0;

    }

    .welcome-text > small {
      /* a guarenteed chance to a happier life */

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 14px;
      line-height: 27px;

      /* Gray 5 */
      color: #E0E0E0;

    }

    .welcome-text br {
      display: none;
    }

    .menu-items > div {
      display: none;
    }

    .d-none {
      display: none !important;
    }

    @media (max-width: 800px) {
      #landing-page > div:nth-child(1) {
          grid-column: 1/2;
          padding: 1rem;
      }

      #landing-page > div:nth-child(2) {
          grid-column: 2/-1;
          margin: 1rem 1rem auto auto;
          white-space: nowrap;
          font-size: 15px;
          line-height: 146.4%;
      }

      .sliders {
        padding-left: 1rem;
      }

      #landing-page > div:nth-child(5) {
        margin-right: 1rem;
      }

      .social-media-icons > div {
        margin: -3px 0;
        transform: scale(0.7);
      }

      #landing-page > div:nth-child(6) {
        padding-left: 21px;
        padding-bottom: 1rem;
      }

      #landing-page > div:nth-child(7) {
        padding-bottom: 1rem;
      }

      #landing-page > div:nth-child(8) {
        padding: 0 1rem 1rem 0;
      }

      .welcome-text > p:nth-child(1) {

        font-size: 24px;
        line-height: 29px;

      }

      .welcome-text > p:nth-child(2) {
        /* NLP! */
        font-size: 61px;
        line-height: 74px;
      }

      .welcome-text > small {
        font-size: 15px;
        line-height: 155.4%;
        font-weight: normal;
      }

      .menu-items > li {
        display: none;
      }

      .menu-items > div {
        display: block;
        align-self: flex-end;
      }

      .welcome-text br {
        display: block;
      }

    }

    .flag.inactive > svg:nth-child(2){
      display: none;
    }

    .flag.active > svg:nth-child(1) {
      display: none;
    }

    .welcome-text > a {
      text-decoration: none;
      color: #f2f2f2;

      padding: 2px 8px;
      margin: 8px 8px;
      border-radius: 2px;
      background: #333333;

      font-family: Montserrat;
      font-style: normal;
      font-weight: 100;
      font-size: 13px;
      line-height: 211.9%;
    }

    </style>
