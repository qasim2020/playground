    $(document).ready( function() {
      $('.custom-container').height(window.innerHeight);
      if (window.innerHeight < 690) {
        $('section').css('min-height',window.innerHeight);
        $('section').removeClass('d-none');
      } else if (window.innerWidth < 700) {
        $('section').css('min-height',window.innerHeight);
        $('section').removeClass('d-none');
      } else {
        $('section').css('height',window.innerHeight);
        $('section').not(':eq(0)').addClass('d-none');
      }

    })

    window.onresize = function() {

        $(document).trigger('ready');

    }


    let info = {};

    var ts;
    $('*').on('touchstart', function (e){
      return;
       ts = e.originalEvent.touches[0].clientY;
       info.current = $(e.target).closest('section').attr('id');
       // console.log(info.current);
    });

    $('*').on('touchend', function (e){
      return;
       var te = e.originalEvent.changedTouches[0].clientY;
       console.log(ts,te);
       if(ts > te+50){
         info.didScroll = true
         info.up = false;
       } else if(ts < te-50){
         info.didScroll = true
         info.up = true;
       }
    });

    $('*').on('mousewheel',function(event){
      // var pageX = (event.type.toLowerCase() === 'mousewheel') ? event.pageX : event.originalEvent.touches[0].pageX;
      console.log('mousewhee;');
      if ($(this).closest('section').attr('id')) info.current = $(this).closest('section').attr('id');
      info.didScroll = true;
      console.log(info.current);
      info.up = event.originalEvent.wheelDelta / 120 > 0;
    });

    setInterval(function() {
        if (window.innerHeight < 690) return console.log('returning');

        if ( info.didScroll ) {
            info.didScroll = false;
            console.log(info);
            // if (info.current == info.oldcurrent) return console.log('dont scroll yet');
            if (info.up) {
              if (!$(`#${info.current}`).prevAll('section:eq(0)').length) return console.log('no more content up there');
              $(`#${info.current}`).addClass('d-none');
              $(`#${info.current}`).prevAll('section:eq(0)').removeClass('d-none');
              let index = $( "section" ).index( $(`#${info.current}`).prevAll('section:eq(0)') );
              $('#active_slider').insertBefore( $(`.sliders > svg:eq(${index})`) );
            }
            else {
              if (!$(`#${info.current}`).nextAll('section:eq(0)').length) return console.log('no more content down there');
              $(`#${info.current}`).addClass('d-none');
              $(`#${info.current}`).nextAll('section:eq(0)').removeClass('d-none');
              let index = $( "section" ).index( $(`#${info.current}`).nextAll('section:eq(0)') );
              $('#active_slider').insertAfter( $(`.sliders > svg:eq(${index})`) );
            }
            info.oldcurrent = info.current;
        }
    }, 10);

