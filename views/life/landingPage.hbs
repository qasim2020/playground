        <a class="selector {{#if (match "n" data.pjaxId)}}active{{/if}}" id="showBlogs"  href="/{{data.brand}}/gen/pjax/blogs/landingPage" data-pjax>Blogs</a>
        <a class="selector" id="blogStreak" href="/{{data.brand}}/gen/pjax/blogStreak/landingPage" data-pjax>Streak</a>
        <a class="selector" id="newsletters" href="/{{data.brand}}/gen/pjax/newsletters/landingPage" data-pjax>Newsletters</a>
        <button class="selector" onclick="showModel()">Subscribe</button>

      </div>

      <div class="content" id="pjax-container">
      
            {{#if (match "n" data.pjaxId)}}

            <div class="blogs">

                {{#each data.blogs}}
                <div class="card" slug="{{slug}}" onclick="openBlog(this)">

                        <h4>{{index}}</h4>
                        <h1>{{body.0.msg}}</h1>
                        <h5>PUBLISHED ON</h5>
                        <p>{{date}}</p>
                    
                </div>
                {{/each}}


            </div>

            {{/if}}

      </div>

