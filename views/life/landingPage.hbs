    <style media="screen">
    /* font-family: 'Merriweather', serif; */
    /* font-family: 'Playfair Display', serif; */
    /* font-family: 'Roboto', sans-serif; */

    .container {
      /* margin-top: -200px; */
    }

    .container > h1 {
      font-family: 'Merriweather', serif;
    }

    p {
      font-family: 'Roboto', sans-serif;
    }

    .logo-area {
      display: flex;
      height: 44px;
      align-items: center;
      padding: 0 20px;
      cursor: pointer;
      max-width: 1400px;
      margin: auto;
    }

    .logo-area > p:hover {
      color: black;
    }

    .logo-area > p {
      padding-left: 10px;
      margin-left: auto;
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 14px;
      line-height: 157.9%;
      display: flex;
      align-items: center;
      color: #4F4F4F;
    }

    .container {
      padding: 0 20px;
      display: flex;
      /* min-height: 90vh; */
    }

    .left-part {
      padding: 0 34px;
      position: -webkit-sticky;
      position: sticky;
      height: 60vh;
      top: 10px;
    }

    .left-part > * {
      max-width: 258px;
    }

    .left-part > svg {
      position: absolute;
      z-index: -10;
    }

    .left-part > p:nth-child(2) {
      font-family: Roboto;
      font-style: normal;
      font-weight: bold;
      font-size: 20px;
      line-height: 157.9%;
      display: flex;
      align-items: center;
      color: #4F4F4F;
      padding: 20px 0 1px 0;
      max-width: 268px;
      margin-bottom: 30px;
    }

    .left-part > .search {
        position: relative;
    }

    .left-part > .search > svg {
        position: absolute;
        right: 17px;
        top: 50%;
        transform: translate(0%, -50%);
    }

    .left-part > .selector.pending {
	color: brown;
    }

    .left-part > .selector {
        margin: 0;
        padding: 10px 0;
        padding-left: 18px;
        font-size: 16px;
        color: #4F4F4F;
        font-family: 'Roboto';
        font-weight: 300;
        display: block;
    }

    .left-part > button.selector {
        background: white;
        border: 1px solid white;
        width: 100%;
        text-align: left;
    }

    .left-part > .selector:hover {
        background: whitesmoke;
        cursor: pointer;
    }

    .left-part > .selector.active {
        color: orange;
    }

    .left-part > .selector.active::after {
        content: "   »"
    }

    .left-part > a {
        text-decoration: none;
        color: inherit;
        display: block;
    }

    .left-part > p > input {
        width: 100%;
        padding: 18px;
        box-sizing: border-box;
        border: 1px solid #4F4F4F;
        font-size: 16px;
        color: #4F4F4F;
        font-family: 'Roboto';
        font-weight: 300;
    }

    .muted {
      font-family: Roboto;
      font-style: normal;
      font-weight: normal;
      font-size: 20px;
      line-height: 157.9%;

      /* Gray 3 */
      color: #828282;

    }

    .muted > a {
      font-style: normal;
      color: black;
      font-weight: lighter;
    }

    .content {
        width: 100%;
        box-sizing: border-box;
        padding: 0 20px;
        height: fit-content;
    }

    .content > .blogs {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    }

    .card {
        font-size: 80%;
        min-width: 150px;
        padding: 15px;
        box-sizing: border-box;
        word-break: break-word;
        background: whitesmoke;
        margin: 5px;
        border-radius: 5px;
        font-family: "Roboto";
        display: flex;
        flex-direction: column;

    }

    .letter {
        border: 1px dashed orange;
    }

    .card:hover {
        background: white;
        cursor: pointer;
    }

    .card > h4 {
        color: #4F4F4F;
        margin: 12px 0;
    }

    .card > h1 {
        margin-bottom: auto;
        color: #4F4F4F;
        background: #219653;
        box-sizing: border-box;
        transform: rotate( -1deg );
        color: white;
        padding: 8px;
    }

    .letter > h1 {
        background: orange;
    }

    .card > h5 {
        padding: 2px 4px;
        margin-top: 22px;
        margin-bottom: 0px;
        font-weight: 300;
    }

    .card > p {
        padding: 7px 4px 2px 4px;
        margin-top: 0;
        font-weight: bold;
        font-size: 14px;
        color: #4F4F4F;
    }

    .right-part {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(100px, 1fr));
      grid-template-rows: repeat(auto,100px);
    }

    .right-part > span {
      background: #F2F2F2;
      border: 1px solid #E0E0E0;
      height: 100px;

      font-family: Roboto;
      font-style: normal;
      font-weight: bold;
      font-size: 30px;
      line-height: 157.9%;
      text-align: center;
      display: flex;
      justify-content: center;
      align-items: center;
      color: #BDBDBD;

    }

    .right-part > span.active {
      background: #219653;
      color: white;
      border: 1px solid #E0E0E0;
      cursor: pointer;
    }

    .right-part > span.active:hover {
      background: green;
    }

    /**
    .left-part > a {
      font-family: Roboto;
      font-style: normal;
      font-weight: normal;
      font-size: 12px;
      line-height: 157.9%;
      display: flex;
      align-items: center;
      max-width: 171px;
      color: #219653;
      margin: auto;
      border: 1px solid #219653;
      padding: 10px;
      margin-top: 15px;
    }
    **/

    .left-part > a:hover {
      background: #f2f2f2;
    }

    .left-part > a > svg {
      margin-top: -5px;
      padding-left: 13px;
    }

    .left-part > a > span {
      width: 70%;
      margin: auto;
    }

    .locked {
      opacity: 0.3;
      pointer-events: none;
      cursor: not-allowed;
    }

    .note {
      padding: 24px;
      background: #323232;
      color: white;
      max-width: 1400px;
      margin: auto;
      margin-bottom: 1rem;
    }

    .note > p {
      color: lightgrey;
      line-height: 1.5;
      font-size: 18px;
    }

    @media (max-width:799px) {
      .container {
        /* padding: 0 20px; */
        display: block;
        /* min-height: 90vh; */
      }

      .left-part {
        padding: 0;
        padding-bottom: 18px;
        width: 100%;
        position: relative;
        height: auto;
      }

      .left-part > a {
        max-width: 100%;
        margin: inherit;
        margin-bottom: 17px;
        margin-top: 13px;
      }

      .left-part > svg {
        position: absolute;
        z-index: -10;
        left: 0;
        top: -52px;
      }

      .content {
          padding: 0;
      }

      .right-part {
        padding: 0;
        width: auto;
      }

      iframe {
        display: none;
      }
    }

    @media (max-width: 375px) {
        .left-part > * {
            max-width: 375px;
        }
    }

    </style>

    <title>Qasim's Diary</title>

