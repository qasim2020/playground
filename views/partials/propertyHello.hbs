<!-- Primary Meta Tags -->
<title>{{data.owner.brandName}}</title>
<meta name="title" content="{{data.owner.brandName}}">
<meta name="description" content="{{data.owner.brandDesc}}">

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="{{data.owner.brandWebsite}}">
<meta property="og:title" content="{{data.owner.brandName}}">
<meta property="og:description" content="{{data.owner.brandDesc}}">
<meta property="og:image" content="{{data.owner.metaImg}}">

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:url" content="{{data.owner.brandWebsite}}">
<meta property="twitter:title" content="{{data.owner.brandName}}">
<meta property="twitter:description" content="{{data.owner.brandDesc}}">
<meta property="twitter:image" content="{{data.owner.metaImg}}">
