    body {
        font-family: "Roboto", sans-serif;
        padding: 0;
        margin: 0;
    }

    body>*:not(.slide-footer) {
        max-width: 900px;
        margin: auto;
    }

    .layerOne {
        padding-top: 51px;
    }

    .layerOne>* {
        padding: 0 10px;
    }

    .container {
        padding-bottom: 10px;
    }

    nav {
        z-index: 100;
        display: flex;
        justify-content: space-between;
        position: fixed;
        top: 0;
        box-sizing: border-box;
        background: white;
        width: 100%;
        max-width: 900px;
        left: 50%;
        transform: translate(-50%);
        padding: 0 18px !important;
    }

    nav>.right {
        display: flex;
        align-items: center;
    }

    .right>.svg {
        margin-left: 4px;
        z-index: 100;
    }

    .drawForm>.layerOne>nav>.right>.svg {
        margin-top: -4px;
    }

    .drawForm>.formBox>form>.btn.close {
        display: none;
    }

    .sliders {
        padding: 34px 7px;
        background: black;
        display: flex;
        overflow: hidden;
        position: relative;
        box-sizing: border-box;
    }

    .sliders>div {
        min-width: 100%;
        height: 500px;
        position: relative;
        box-sizing: border-box;
        color: white;
        display: none;
        font-weight: bold;
    }

    .sliders>div.active {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
    }

    .sliders>div>p:nth-child(1) {
        font-size: 13vh;
        line-height: 1.5;
        margin: 0;
        padding: 0;
    }

    .sliders>div>p:nth-child(2) {
        position: absolute;
        bottom: 10px;
        color: lightslategray;
        font-weight: 300;
    }

    .sliders>div>div {
        height: 100%;
    }

    .movers {
        background: black;
        display: flex;
        align-items: center;
        justify-content: space-evenly;
        padding: 0px 0 20px 0;
    }

    .movers button {
        padding: 8px 11px;
        background: #272727;
        border: #272727;
        color: dimgrey;
    }

    .movers button.active {
        background: white;
        color: black;
    }

    .svg>svg {
        margin-top: 4px;
    }

    .svg>.content {
        position: absolute;
        left: 0;
        padding: 14px 10px;
        display: flex;
        flex-direction: column;
        align-items: flex-end;
        width: 100%;
        box-sizing: border-box;
        top: 48px;
        background: #F2F2F2;
        border-bottom: 1px dashed black;
    }

    .svg>svg:nth-child(1) {
        display: none;
    }

    .svg>svg:nth-child(2) {
        display: inline-block;
        border: 1px solid black;
        box-sizing: border-box;
    }

    .close.svg>.content {
        display: none;
    }

    .close.svg>svg:nth-child(2) {
        display: none;
    }

    .close.svg>svg:nth-child(1) {
        display: inline-block;
        box-sizing: border-box;
    }

    .locations {
        line-height: 1.5;
        color: darkslategrey;
    }

    .box-hdg {
        font-size: 12px;
        font-weight: bold;
        padding-left: 7px;
        margin-bottom: 7px;
    }

    .box-hdg>span {
        color: slategrey;
    }

    .banner>p {
        line-height: 1.5;
        font-weight: 500;
        margin-top: 0px;
        font-size: 13px;
        color: black;
        font-weight: normal;
    }

    .btn {
        display: inline-block;
        padding: 5px 9px 4px 9px;
        background: #FFFFFF;
        border: 1px solid #333333;
        box-sizing: border-box;
        font-weight: bold;
        font-size: 14px;
        color: #333333;
        margin: 2px 0px;
        text-decoration: none;
    }

    .btn.blue {
        background: #2F80ED;
        color: white;
    }

    .btn.red {
        background: #e85f55;
        color: white;
    }

    a.viewMore {
        width: fit-content;
        margin: -4px 0px 10px 10px;
        background: black;
        border: 1px solid black;
        color: white;
        padding: 4px 6px;
        font-weight: bold;
        cursor: pointer;
        font-size: 10px;
        text-decoration: none;
    }

    button.fixed {
        position: fixed;
        right: 4px;
        bottom: 4px;
        background: lightblue;
        border: lightblue;
        padding: 8px 8px;
        color: darkslategray;
        font-weight: bold;
        border: 2px solid black;
        color: black;
    }

    .thumbnail {
        height: 180px;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        background: lightcoral;
        color: white;
        font-weight: bold;
        font-size: 25px;
    }

    .px-10 {
        padding-left: 10px;
        padding-right: 10px;
    }

    .cards {
        margin-top: 15px;
        display: grid;
        grid-template-columns: 1fr 1fr;
        grid-gap: 10px;
    }

    .temporary {
        padding: 0 10px 10px 10px;
    }

    .cards>p {
        grid-column: 1/-1;
        font-weight: bold;
        align-items: center;
        justify-content: center;
        margin: auto;
        margin-top: 20px;
    }

    .card {
        line-height: 1.5;
        font-weight: bold;
        color: #333333;
        border: 3px solid #f5f5f5;
        display: flex;
        flex-direction: column;
    }

    .card:hover {
        cursor: pointer;
        opacity: 0.8;
    }

    .card>div {
        padding: 10px;
        background: #f5f5f5;
    }

    .card>div>p {
        margin-bottom: 0px;
    }

    .card>div>p.heavy {
        font-size: 23px;
        font-weight: bolder;
    }

    .card>p {
        flex-grow: 1;
        padding: 10px;
    }

    .card.select {
        border: 3px solid #333333;
    }

    .msgBox>textarea {
        width: 90%;
        font-size: 16px;
        border: 0px;
        color: darkslategray;
        font-weight: bold;
        font-family: inherit;
        word-spacing: 2px;
        line-height: 1.5;
    }

    .msgBox>button {
        background: lightblue;
        padding: 4px 8px;
        border: 2px solid black;
        font-size: 16px;

    }

    .d-none {
        display: none !important;
    }

    .btn.small {
        font-size: 12px;
        width: fit-content;
    }

    .btn.active.selling {
        background: #219653;
        color: white;
    }

    .btn.active.sold {
        background: #4F4F4F;
        color: #6FCF97;
        border-color: black;
    }

    .btn.active.required {
        background: #EB5757;
        color: white;
    }

    .btn.active.archive {
        background: #333333;
        color: white;
    }

    .btn.active {
        background: #333333;
        color: white;
        border-color: black;
        font-weight: normal;
    }

    .btn.thumbnail {
        font-size: 12px;
        background: white;
        color: lightcoral;
        border: lightcoral 2px solid;
    }

    .firstBox {
        padding-top: 10px;
        border-top: 1px dashed;
        border-bottom: 1px dashed;
        padding-bottom: 10px;
        padding-left: 18px;
    }

    .firstBox>button {
        content: "CLOSE FILTER";
    }

    .firstBox.close>button {
        content: "OPEN FILTER"
    }

    .firstBox.close>.filters {
        display: none;
    }

    .btn.filter>span:nth-child(1) {
        display: block;
    }

    .btn.filter>span:nth-child(2) {
        display: none;
    }

    .btn.filter.active>span:nth-child(1) {
        display: none;
    }

    .btn.filter.active>span:nth-child(2) {
        display: block;
    }

    .filters>.btn:hover,
    .btn.filter {
        cursor: pointer;
    }

    .note {
        font-size: 12px;
        line-height: 1.5;
        padding: 0 18px;
        color: lightslategrey;
    }

    .box-top h1 {
        font-size: 1.5rem;
        font-weight: 200;
    }

    .box.note {
        line-height: 1.5;
        font-weight: 300;
        color: lightslategray;
        font-size: inherit;
    }

    .note.black {
        font-size: 16px;
        margin: 0px;
        padding: 18px;
        color: lightslategray;
        background: black;
        font-weight: 300;
    }


    .formBox {
        padding-bottom: 40px;
    }

    .box-top {
        z-index: 10;
        background: black;
        padding: 21px 27px 27px 27px;
        margin: 0 10px;
        color: white;
    }

    .box-top>* {
        padding: 0px;
    }

    form {
        display: flex;
        flex-direction: column;
        width: 100%;
        box-sizing: border-box;
    }

    form>label {
        margin-top: 25px;
        margin-bottom: 12px;
        padding: 0 27px;
        font-size: 13px;
        font-weight: bold;
        color: #2F80ED;
    }

    form>input,
    form>textarea {
        outline: 0px;
        border: 0px;
        background: #2F80ED;
        padding: 28px 27px;
        font-size: 19px;
        color: white;
        font-family: inherit;
        line-height: 1.5;
        font-weight: 300;
    }

    form>.note {
        background: #2f80ec;
        margin-top: -17px;
        border: 0px solid #2f80ec;
        color: #ffffffab;
        padding: 11px 27px 24px 27px;
        z-index: 10;
        line-height: 1.5;
    }

    form>.btn-note {
        color: lightslategrey;
        padding: 0 26px;
        z-index: 10;
        font-size: 13px;
        line-height: 1.5;
    }

    form>input::placeholder,
    form>textarea::placeholder {
        color: lightblue;
    }


    form>.mt-24 {
        margin-top: 24px;
    }

    form>.btn {
        margin-left: 24px;
        display: block;
        width: fit-content;
    }

    form>.absolute-btns {
        background: #2F80ED;
        padding: 26px;
    }

    .cards>.page {
        padding: 10px 20px 20px 20px;
        line-height: 1.5;
        color: #333333;
        border: 3px solid #f5f5f5;
    }

    .page:hover {
        cursor: pointer;
        border-color: #333333;
    }

    .page>p {
        color: #333333;
        font-weight: 300;
        font-size: 12px;
    }

    .page>h1 {
        font-size: 1.6rem;
        font-weight: bolder;
        color: #333333;
        line-height: 1.2;
    }


    .page-nav {
        display: flex;
        align-items: center;
    }

    .page-nav>a {
        display: inline-block;
        padding-right: 7px;
    }

    .ticket {
        background: whitesmoke;
        padding: 27px;
        line-height: 1.5;
        max-width: 500px;
        margin: auto;
        border: 4px dashed lightgray;
    }

    .ticket>.timestamp {
        font-size: 13px;
        padding-bottom: 14px;
        max-width: 220px;
        border-bottom: 2px solid black;
    }

    .ticket>.ticketNo {
        display: inline-block;
        background: darkcyan;
        color: white;
        padding: 0 27px;
    }

    .ticket>.info {
        font-weight: bold;
        font-size: 13px;
        color: darkcyan;
        margin-bottom: 0px;
    }

    .slides {
        padding: 10px;
    }

    .slides>div {
        height: 200px;
        background: whitesmoke;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .slide-editor,
    .slide-new {
        display: flex;
        flex-direction: column;
    }

    .slide {
        display: flex;
        flex-grow: 1;
        border: 6px solid lightcoral;
        margin-bottom: -40px;
    }

    .slide>div {
        width: 100%;
    }

    .placeholder {
        width: 100%;
        background: lightcoral;
        border-top: 2px solid lightcoral;
        border-bottom: 2px solid lightcoral;
        color: white;
        box-sizing: border-box;
        margin-top: 10px;
    }

    .placeholder>p {
        display: flex;
        align-items: center;
        font-weight: bold;
        font-size: 15px;
        padding: 0px;
        margin: 0px;
        width: 100%;
        box-sizing: border-box;
    }

    .placeholder>p>span {
        font-weight: bold;
        font-size: 12px;
        padding: 3px 11px;
    }

    .placeholder>p>input {
        font-weight: bold;
        width: 100%;
        padding: 13px 10px;
        font-size: 15px;
        border: 1px solid transparent;
    }

    .btn.white {
        border: 1px solid transparent;
    }

    .w-100 {
        width: 100%;
    }

    .h-100 {
        height: 100%;
    }

    .btn#mode {
        position: fixed;
        bottom: -2px;
        right: 0px;
        background: lightcoral;
        color: white;
        border: 0px;
        padding: 0px 7px 6px 7px;
    }

    .advanced-editor {
        flex-grow: 1;
        display: flex;
        margin-bottom: -40px;
    }

    .advanced-editor>textarea {
        width: 100%;
        border: 4px solid lightcoral;
        padding: 10px;
        background: darkcyan;
        color: white;
        font-size: 12px;
        font-family: system-ui;
    }

    .CodeMirror {
        z-index: 0;
        width: 100%;
        height: 100%;
        border: 4px solid lightcoral;
        box-sizing: border-box;
    }

    {{> cssSnippets/propertyTemplates}}

    .slide {
        position: relative
    }

    .slide-review {
        background: black;
        padding: 20px;
        height: auto !important;
    }

    .slide-review>div {
        display: flex;
        flex-direction: column;
    }

    .slide-review>div>.banner>p {
        color: white;
        font-size: 20px;
        word-spacing: 30px;
        font-weight: bold;
        max-width: 150px;
    }

    .slide-review>div>.quote>img {
        width: 50px;
    }

    .slide-review>div>.quote>div {
        max-width: 68vw;
        width: 400px;
    }

    .slide-review>div>.quote>div>p {
        font-size: 16px;
        line-height: 1.5;
        font-weight: 300;
        color: lightslategray;
    }

    .slide-contact {
        padding: 20px;
        background: black;
    }

    .slide-contact>div {
        height: 100%;
    }

    .slide-contact>div>.banner {}

    .slide-contact>div>.banner>p {
        font-size: 20px;
        color: white;
        font-weight: bold;
        text-align: center;
    }

    .slide-contact>div>.img {
        height: 85%;
        width: 100%;
        position: relative;
        display: flex;
        align-items: center;
    }

    .slide-contact>div>.img>img {
        height: 90%;
        position: absolute;
        border: 4px solid white;
        left: 50%;
        transform: translate(-50%);
        margin-top: -17px;
    }

    .ck-content {
        max-width: 900px;
        margin: auto;
        color: #333333;
    }

    .ck-content p,
    .ck-content ul li,
    figcaption {
        font-weight: 400;
        line-height: 1.5;
        font-size: 19px;
    }

    .ck-content h2 {
        font-size: 3.5rem;
        font-weight: 800;
    }

