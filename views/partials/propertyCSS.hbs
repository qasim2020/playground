    body {
        font-family: "Roboto", sans-serif;
        padding: 0;
        margin: 0;
    }

    .layerOne>* {
        padding: 0 10px;
    }


    nav {
        padding: 0 14px;
        display: flex;
        justify-content: space-between;
    }

    nav>.right {
        display: flex;
        align-items: center;
    }

    .right>.svg {
        margin-left: 4px;
        z-index: 100;
    }

    .svg>svg {
        margin-top: 4px;
    }

    .svg>.content {
        position: absolute;
        left: 0;
        padding: 14px 10px;
        display: flex;
        flex-direction: column;
        align-items: flex-end;
        width: 100%;
        box-sizing: border-box;
        top: 48px;
        background: #F2F2F2;
        border-bottom: 1px dashed black;
    }

    .svg>svg:nth-child(1) {
        display: none;
    }

    .svg>svg:nth-child(2) {
        display: inline-block;
        border: 1px solid black;
        box-sizing: border-box;
    }

    .close.svg>.content {
        display: none;
    }

    .close.svg>svg:nth-child(2) {
        display: none;
    }

    .close.svg>svg:nth-child(1) {
        display: inline-block;
        box-sizing: border-box;
    }



    .locations {
        line-height: 1.5;
        color: darkslategrey;
    }

    .box-hdg {
        font-size: 12px;
        font-weight: bold;
        padding-left: 7px;
        margin-bottom: 7px;
    }

    .banner>p {
        line-height: 1.5;
        /* color: #ff9800; */
        font-weight: 500;
        margin-top: 0px;
        font-size: 13px;
        color: black;
        font-weight: normal;
    }

    .btn {
        display: inline-block;
        padding: 5px 9px 4px 9px;
        background: #FFFFFF;
        border: 1px solid #333333;
        box-sizing: border-box;
        font-weight: bold;
        font-size: 14px;
        color: #333333;
        margin: 2px 0px;
        text-decoration: none;
    }

    .btn.blue {
        background: #2F80ED;
        color: white;
    }

    button.fixed {
        position: fixed;
        right: 4px;
        bottom: 4px;
        background: lightblue;
        border: lightblue;
        padding: 8px 8px;
        color: darkslategray;
        font-weight: bold;
        border: 2px solid black;
        color: black;
    }

    .cards {
        margin-top: 15px;
        display: grid;
        grid-template-columns: 1fr 1fr;
        grid-gap: 10px;
    }

    .cards>p {
        grid-column: 1/-1;
        font-weight: bold;
        align-items: center;
        justify-content: center;
        margin: auto;
        margin-top: 20px;
    }

    .card {
        padding: 0 14px 15px 14px;
        line-height: 1.5;
        font-weight: bold;
        color: #333333;
        background: #f5f5f5;
        border: 3px solid #f5f5f5;

        display: flex;
        flex-direction: column;
    }

    .card>p {
        flex-grow: 1;
    }

    .card.select {
        border: 3px solid #333333;
    }

    .msgBox {
        position: fixed;
        z-index: 100;
        background: white;
        top: 0;
        left: 0;
        width: 100vw;
        height: 100vh;
    }

    .msgBox>textarea {
        width: 90%;
        font-size: 16px;
        border: 0px;
        color: darkslategray;
        font-weight: bold;
        font-family: inherit;
        word-spacing: 2px;
        line-height: 1.5;
    }

    .msgBox>button {
        background: lightblue;
        padding: 4px 8px;
        border: 2px solid black;
        font-size: 16px;

    }

    .d-none {
        display: none;
    }

    .btn.small {
        font-size: 12px;
        width: fit-content;
    }

    .btn.active.selling {
        background: #219653;
        color: white;
    }

    .btn.active.sold {
        background: #4F4F4F;
        color: #6FCF97;
        border-color: black;
    }

    .btn.active.required {
        background: #EB5757;
        color: white;
    }

    .btn.active.archive {
        background: #333333;
        color: white;
    }

    .btn.active {
        background: #333333;
        color: white;
        border-color: black;
        font-weight: normal;
    }

    .firstBox {
        padding-top: 10px;
        border-top: 1px dashed;
        border-bottom: 1px dashed;
        padding-bottom: 10px;
    }

    .firstBox>button {
        content: "CLOSE FILTER";
    }

    .firstBox.close>button {
        content: "OPEN FILTER"
    }

    .firstBox.close>.filters {
        display: none;
    }

    .btn.filter>span:nth-child(1) {
        display: block;
    }

    .btn.filter>span:nth-child(2) {
        display: none;
    }

    .btn.filter.active>span:nth-child(1) {
        display: none;
    }

    .btn.filter.active>span:nth-child(2) {
        display: block;
    }

    .note {
        font-size: 12px;
        line-height: 1.5;
        padding: 0 18px;
        color: lightslategrey;
    }

    .note.black {
        font-size: 16px;
        margin: 0px;
        padding: 18px;
        color: white;
        background: black;
    }

    form>.note {
        padding: 0 24px;
    }


    .formBox {
        width: 100vw;
        box-sizing: border-box;
        padding: 0px 0px;
        margin-bottom: 40px;
    }

    .formBox>p {
        margin-top: 29px;
        padding: 0 27px;
    }

    .formBox>.note {
        color: lightslategrey;
    }

    form {
        display: flex;
        flex-direction: column;
        width: 100%;
        box-sizing: border-box;
    }

    form>label {
        margin-top: 25px;
        margin-bottom: 12px;
        padding: 0 27px;
        font-size: 13px;
        font-weight: bold;
        color: #2F80ED;
    }

    form>input,
    form>textarea {
        border: 0px;
        background: #2F80ED;
        padding: 28px 27px;
        font-size: 19px;
        color: white;
        font-family: inherit;
        line-height: 1.5;
        font-weight: 300;
    }

    form>input::placeholder,
    form>textarea::placeholder {
        color: lightblue;
    }


    form>.mt-24 {
        margin-top: 24px;
    }

    form>.btn {
        margin-left: 24px;
        display: block;
        width: fit-content;
    }

    form>.absolute-btns {
        background: #2F80ED;
        padding: 26px;
    }

    .cards>.page {
        box-shadow: 0 0px 30px darkgrey;
        border-radius: 5px;
        padding: 10px 20px 20px 20px;
        line-height: 1.5;
    }

    .page-nav {
        display: flex;
        align-items: center;
    }

    .page-nav>a {
        display: inline-block;
        padding-right: 7px;
    }
