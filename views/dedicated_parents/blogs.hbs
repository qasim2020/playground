<!--[if IE 8]> <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
    <head>
        <title>Dedicated Parents | Blogs</title>
        {{> dedicated_parents/header}}
    </head>
    <body>

        {{> dedicated_parents/headerNav active='blogs'}}

		<div class="container">
			
			<div class="page-header">
				<div class="row">
					<div class="col-md-6 col-sm-6">
						<h2 class="page-title">Our Blogs</h2>
					</div> <!-- /.col-md-6 -->
					<div class="col-md-6 col-sm-6 hidden-xs back-home">
						<a href="index.html">&larr; Go back Home</a>
					</div> <!-- /.col-md-6 -->
				</div> <!-- /.row -->
			</div> <!-- /.page-header -->

			<div class="row">
				
				<div class="col-md-8">
                                    {{#each data.blogs}}
					<div class="blog-list">
						<div class="post-thumb">
                                        <img src="https://res.cloudinary.com/miscellaneous/image/upload/c_lpad,h_242,w_300/{{this.number}}/dedicatedparents/blogs-photos/{{this.imgSlug}}" alt="">
						</div> <!-- /.blog-thumb -->
						<div class="post-content">
                                                    <h4 class="post-title"><a href="/dedicated_parents/gen/page/d_pblog/{{this.slug}}">{{this.title}}</a></h4>
                                                    <p class="post-meta"><a href="#">{{this.date}}</a> with <a href="#">4 Comments</a></p>
                                                    <p>{{this.subtitle}}</p>
                                                    <a href="/dedicated_parents/gen/page/d_pblog/{{this.slug}}" class="read-more">Continue Reading &rarr;</a>
						</div> <!-- /.blog-content -->
					</div> <!-- /.blog-list -->
                                    {{/each}}
				</div> <!-- /.col-md-8 -->

				<div class="col-md-4">
                                        {{> dedicated_parents/galleryPhotos}}
                                        {{> dedicated_parents/upcomingEvents}}
				</div> <!-- /.col-md-4 -->

			</div> <!-- /.row -->

		</div> <!-- /.container -->

		
                {{> dedicated_parents/footer}}
	

    </body>
</html>
