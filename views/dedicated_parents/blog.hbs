    <body>
	
                {{> dedicated_parents/headerNav active='blogs'}}

		<div class="container">
			
			<div class="page-header">
				<div class="row">
					<div class="col-md-6 col-sm-6">
						<h2 class="page-title">Post Single</h2>
					</div> <!-- /.col-md-6 -->
					<div class="col-md-6 col-sm-6 hidden-xs back-home">
						<a href="index.html">&larr; Go back Home</a>
					</div> <!-- /.col-md-6 -->
				</div> <!-- /.row -->
			</div> <!-- /.page-header -->

			<div class="row">
				
				<div class="col-md-8">
					<div class="blog-single">
						<div class="post-image">
							<img src="http://placehold.it/770x400" alt="">
							<div class="image-over">
								<span><i class="fa fa-user"></i>Esmet Hajrizi</span>
								<span><i class="fa fa-comments-o"></i>Comments <em>(3)</em></span>
								<span><i class="fa fa-calendar-o"></i>24 January 2014</span>
							</div>
						</div> <!-- /.blog-thumb -->
						<div class="post-content">
                                                    {{{data.blog.post}}}
							<div class="tags">
								<span><i class="fa fa-tags"></i></span>
                                                                {{#each (splitComma data.blog.tags)}}
                                                                <a href="#">{{this}}</a>
                                                                {{/each}}
							</div> <!-- /.tags -->
						</div> <!-- /.blog-content -->
					</div> <!-- /.blog-single -->
					<div class="row">
						<div class="col-md-12">
							<a href="javascript:void(0);" class="go-prev"><i class="fa fa-long-arrow-left"></i>Prev Blog</a>
							<a href="javascript:void(0);" class="go-next">Next Blog <i class="fa fa-long-arrow-right"></i></a>
						</div> <!-- /.col-md-12 -->
					</div> <!-- /.row -->
					<div class="comments box-content">
						<h4 class="widget-title"><span>Comments <em>(3)</em></span></h4>
