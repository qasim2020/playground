					<div class="comment-form box-content">
						<h4 class="widget-title"><span>Leave Us a Comment</span></h4>
						<form>
						    <fieldset>
								<label for="name">First Name: <span>Put your name here</span></label>
								<input type="text" id="name" name="name" class="form-input">
						    </fieldset>
						    <fieldset>
								<label for="email">Email: <span>Put your email here</span></label>
								<input type="email" id="email" name="email" class="form-input">
						    </fieldset>
						    <fieldset>
								<label for="website">Url: <span>Put your website here</span></label>
								<input type="url" id="website" name="website" class="form-input">
						    </fieldset>
						    <fieldset>
								<label for="message">Comment: <span>Feel free to type</span></label>
								<textarea name="message" id="message" rows="6"></textarea>
						    </fieldset>
						    <fieldset>
						    	<button type="submit" id="submit-button" class="btn main-btn">Submit Comment</button>
						    </fieldset>
						</form>
					</div> <!-- /.comment-form -->
				</div> <!-- /.col-md-8 -->

