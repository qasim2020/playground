<!DOCTYPE html>
<!--[if lt IE 7]> 
<html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en">
    <![endif]-->
    <!--[if IE 7]> 
    <html class="no-js lt-ie9 lt-ie8" lang="en">
        <![endif]-->
        <!--[if IE 8]> 
        <html class="no-js lt-ie9" lang="en">
            <![endif]-->
            <!--[if gt IE 8]><!--> 
            <html class="no-js" lang="en">
                <!--<![endif]-->
                <head>
                    <title>Dedicated Parents | Home</title>
                    {{> dedicated_parents/header}}
                </head>
                <body>
                    {{> dedicated_parents/headerNav active='home'}}
                    <div class="container">
                        <div class="top-content">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="u-cause-wrapper hidden-xs">
                                        <span class="u-cause-title">
                                            <a href="#" class="close-u-cause">
                                            <i class="fa fa-times"></i>
                                            </a>
                                            OUR UPDATES 
                                        </span>
                                        <div class="u-cause clearfix">
                                            <dl class="inner-carousel clearfix">
                                                {{#each data.causes}}
                                                <dt></dt>
                                                <dd>
                                                    <a href="/dedicated_parents/gen/page/d_pcause/{{this.slug}}">{{this.title}}</a> |
                                                    {{this.summary}}
                                                </dd>
                                                {{/each}}
                                                {{#each data.events}}
                                                <dt></dt>
                                                <dd>
                                                    <a href="/dedicated_parents/gen/page/d_pevent/{{this.slug}}">{{this.title}}</a> | 
                                                    {{this.summary}}
                                                </dd>
                                                {{/each}}
                                            </dl>
                                            <!-- /.inner-carousel -->
                                        </div>
                                        <!-- /.u-cause -->
                                    </div>
                                    <!-- /.u-cause-wrapper -->
                                </div>
                                <!-- /.col-md-12 -->
                            </div>
                            <!-- /.row -->
                            <div class="slider-wrapper">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="subscribe-form">
                                            <h3 class="widget-title">Stay up to date</h3>
                                            <p>Sign up today for our e-newsletter to find out what is going on and get involved.</p>
                                            <form>
                                                <fieldset>
                                                    <label for="name">First Name:</label>
                                                    <input type="text" id="name" name="name" class="form-input">
                                                </fieldset>
                                                <fieldset>
                                                    <label for="last-name">Last Name:</label>
                                                    <input type="text" id="last-name" name="last-name" class="form-input">
                                                </fieldset>
                                                <fieldset>
                                                    <label for="email">Email:</label>
                                                    <input type="email" id="email" name="email" class="form-input">
                                                </fieldset>
                                                <fieldset>
                                                    <button type="submit" id="send-button" class="btn main-btn">Submit</button>
                                                </fieldset>
                                            </form>
                                        </div>
                                        <!-- /.subscribe-form -->
                                    </div>
                                    <!-- /.col-md-4 -->
                                    <div class="col-md-8">
                                        <div class="flexslider">
                                            <ul class="slides">
                                                {{#each data.events}}
                                                <li>
                                                    <img src="https://res.cloudinary.com/miscellaneous/image/upload/c_lpad,h_550,w_770/v1680079430/dedicatedparents/event-banners/{{this.bannerImg}}" alt="">
                                                    <div class="flex-caption">
                                                        <p>
                                                            {{this.summary}}
                                                            <a href="/dedicated_parents/gen/page/d_pevent/{{this.slug}}">Read More</a>
                                                        </p>
                                                    </div>
                                                </li>
                                                {{/each}}
                                            </ul>
                                        </div>
                                        <!-- /.flexslider -->
                                    </div>
                                    <!-- /.col-md-8 -->
                                </div>
                                <!-- /.row -->
                            </div>
                            <!-- /.slider-wrapper -->
                        </div>
                        <!-- /.top-content -->
                        <div class="services box-content">
                            <div class="row">
                                <div class="col-md-offset-2 col-md-8 services-header">
                                    <h5 class="widget-title">Welcome to Dedicated Parents</h5>
                                    <span></span>
                                    <p>On a journey to craft harmonious homes and flourishing futures, where wisdom lights our way and compassion fuels our choices.</p>
                                </div>
                                <!-- /.services-header -->
                            </div>
                            <!-- /.row -->
                            <div class="row">
                                <div class="col-md-4 service-item">
                                    <div class="service-icon">
                                        <i class="fa fa-globe"></i>
                                    </div>
                                    <div class="service-content">
                                        <h4 class="service-title">{{data.threePages.education.title}}</h4>
                                        <p>{{data.threePages.education.subtitle}}</p>
                                        <a href="/dedicated_parents/gen/page/d_ppage/{{data.threePages.education.slug}}" class="read-more">Read More &rarr;</a>
                                    </div>
                                </div>
                                <!-- /.service-item -->
                                <div class="col-md-4 service-item">
                                    <div class="service-icon">
                                        <i class="fa fa-umbrella"></i>
                                    </div>
                                    <div class="service-content">
                                        <h4 class="service-title">{{data.threePages.helpAndSupport.title}}</h4>
                                        <p>{{data.threePages.helpAndSupport.subtitle}}.</p>
                                        <a href="/dedicated_parents/gen/page/d_ppage/{{data.threePages.helpAndSupport.slug}}" class="read-more">Read More &rarr;</a>
                                    </div>
                                </div>
                                <!-- /.service-item -->
                                <div class="col-md-4 service-item">
                                    <div class="service-icon">
                                        <i class="fa fa-group"></i>
                                    </div>
                                    <div class="service-content">
                                        <h4 class="service-title">{{data.threePages.volunteering.title}}</h4>
                                        <p>{{data.threePages.volunteering.subtitle}}.</p>
                                        <a href="/dedicated_parents/gen/page/d_ppage/{{data.threePages.volunteering.slug}}" class="read-more">Read More &rarr;</a>
                                    </div>
                                </div>
                                <!-- /.service-item -->
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.services -->
                        <div class="last-causes box-content">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="widget-title"><span>Last Cause List</span></h4>
                                </div>
                                <!-- /.col-md-12 -->
                            </div>
                            <!-- /.row -->
                            <div class="row">
                                {{#each data.causes}}
                                <div class="col-md-3 col-sm-6 cause-grid">
                                    <div class="cause-thumb">
                                        <!-- <img src="http://placehold.it/255x200" alt=""> -->
                                        <img src="https://res.cloudinary.com/miscellaneous/image/upload/c_lpad,h_200,w_255/{{this.number}}/dedicatedparents/causes-photos/{{this.imgSlug}}" alt="">
                                        <div class="cause-hover">
                                            <div class="cause-holder clearfix">
                                                <div class="progress">
                                                    <div class="progress-bar" role="progressbar" aria-valuenow="{{this.percent}}" aria-valuemin="0" aria-valuemax="100" style="width: {{this.percent}}%;"></div>
                                                </div>
                                                <span class="raised pull-left">{{this.raised}}</span>
                                                <span class="goal pull-right">{{this.target}}</span>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /.cause-thumb -->
                                    <div class="cause-content">
                                        <h4 class="cause-title"><a href="/dedicated_parents/gen/page/d_pcause/{{this.slug}}">{{this.title}}</a></h4>
                                        <p>{{this.summary}}</p>                                        
                                    </div>
                                    <!-- /.cause-content -->
                                </div>
                                {{/each}}

                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.last-causes -->
                        <div class="row">
                            <div class="col-md-8">
                                <div class="box-content">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h4 class="widget-title"><span>Membership</span></h4>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <img style="width:100%" src="https://res.cloudinary.com/miscellaneous/image/upload/v1694427766/dedicatedparents/banner_landing_page.jpg">
                                        
                                        <!-- /.post-grid -->
                                    </div>
                                    <!-- /.row -->
                                </div>
                                <!-- /.box-content -->
                                <div class="box-content">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h4 class="widget-title"><span>Our team members</span></h4>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12 testimonials-wrapper">
                                            <ul class="testi-tabs clearfix">
                                            {{#each data.staffs}}
                                                <li {{#if (matchValues @index 0)}} class="active" {{/if}}>
                                                    <a href="#testi-content-0{{inc @index}}">
                                                    <img src="https://res.cloudinary.com/miscellaneous/image/upload/c_lpad,h_70,w_70/v1680079430/dedicatedparents/team-photos/{{this.image}}" alt="">
                                                    </a>
                                                </li>
                                            {{/each}}
                                            </ul>
                                            <div class="testi-wrapper">
                                            {{#each data.staffs}}
                                            <div id="testi-content-0{{inc @index}}" class="testi-content {{#if (matchValues @index 0)}}active{{/if}}">
                                                    <div class="testi-author">
                                                        <h6>{{this.title}}<span>{{this.role}}</span></h6>
                                                    </div>
                                                    <p>
                                                        {{reduceStringLength this.detail '151'}}...
                                                    </p>
                                                    <a href="/dedicated_parents/gen/page/d_pstaff/{{this.slug}}">Read More...</a>
                                                </div>
                                            {{/each}}
                                            </div>
                                            <!-- //.testi-wrapper -->
                                        </div>
                                        <!-- /.col-md-12 -->
                                    </div>
                                    <!-- /.row -->
                                </div>
                                <!-- /.box-content -->
                            </div>
                            <!-- /.col-md-8 -->
                            <div class="col-md-4">
                                {{> dedicated_parents/galleryPhotos}}
                                <!-- /.box-content -->
                                {{> dedicated_parents/upcomingEvents}}
                                <!-- /.box-content -->
                            </div>
                            <!-- /.col-md-4 -->
                        </div>
                        <!-- /.row -->
                    </div>
                    {{> dedicated_parents/footer}}
                </body>
            </html>
